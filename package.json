{
  "name": "aryjs",
  "description": "A Node.js wrapper for Eka's Portal.",
  "version": "0.2.2",
  "author": "kokage <k0kag3.k0kag3+github@gmail.com>",
  "scripts": {
    "build": "npm run tsup -- --minify --dts",
    "clean": "shx rm -rf lib",
    "dev": "npm run tsup -- --watch",
    "docs": "typedoc --out docs src && shx touch docs/.nojekyl",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "jest",
    "tsup": "tsup src/index.ts -d lib"
  },
  "types": "lib/index.d.ts",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "dependencies": {
    "date-fns": "^2.21.3",
    "jsdom": "^16.5.3",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/jsdom": "^16.2.10",
    "@types/node": "^15.6.0",
    "@types/node-fetch": "^2.5.10",
    "jest": "^26.6.3",
    "shx": "^0.3.3",
    "ts-jest": "^26.5.6",
    "ts-node": "^9.1.1",
    "tsup": "^4.11.0",
    "typedoc": "^0.20.36",
    "typescript": "^4.2.4"
  },
  "homepage": "https://github.com/k0kag3/aryjs",
  "repository": {
    "type": "git",
    "url": "https://github.com/k0kag3/aryjs.git"
  },
  "bugs": {
    "url": "https://github.com/k0kag3/aryjs/issues"
  },
  "license": "Apache-2.0",
  "keywords": [
    "aryjs"
  ],
  "engines": {
    "node": ">= 12.18.3"
  }
}
